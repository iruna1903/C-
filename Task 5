using System;

namespace Lab1_5 
{
   class Program
    {
        struct RGB
        {
            public byte red;
            public byte green;
            public byte blue;
        };

        RGB black;
        black.red = 0;
        black.green = 0;
        black.blue = 0;

        RGB white;
        white.red = 255;
        white.green = 255; 
        white.blue = 255;

    }
} 
